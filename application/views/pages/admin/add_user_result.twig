{% if ok %}
<p class="shj_ok">These {{ok|length}} user(s) added successfully:</p>
<ol>
	{% for item in ok %}
	<li>Usename: {{ item[0] }} Email: {{ item[1] }} Password: <code>{{ item[2] }}</code> Role: {{ item[3] }}</li>
	{% endfor %}
</ol>
{% endif %}
{% if error %}
<p class="shj_error">Error adding these {{error|length}} user(s):</p>
<ol>
	{% for item in error %}
	<li>Usename: {{ item[0] }} Email: {{ item[1] }} Password: <code>{{ item[2] }}</code> Role: {{ item[3] }} ({{ item[4] }})</li>
	{% endfor %}
</ol>
{% endif %}